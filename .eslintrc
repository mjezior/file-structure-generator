{
  "parser": "babel-eslint",
  "rules": {
    "no-console": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "eqeqeq": 2,
    "guard-for-in": 0,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-undefined": 2,
    "no-unused-vars": [
      2, {
        "vars": "all",
        "args": "after-used"
      }
    ],
    "no-use-before-define": 2,
    "brace-style": [
      2,
      "1tbs", {
        "allowSingleLine": false
      }
    ],
    "camelcase": [
      2, {
        "properties": "always"
      }
    ],
    "comma-spacing": [
      2, {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "func-style": [
      2,
      "declaration"
    ],
    "indent": [
      2,
      2, {
        "SwitchCase": 1
      }
    ],
    "no-multiple-empty-lines": [
      2, {
        "max": 2
      }
    ]
  }
}
